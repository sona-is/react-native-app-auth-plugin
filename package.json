{
  "name": "react-native-app-auth-plugin",
  "version": "1.1.0",
  "description": "Expo config plugin for react-native-app-auth with Swift AppDelegate support",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "prepack": "npm run build",
    "prepare": "npm run build"
  },
  "files": [
    "build",
    "README.md",
    "index.ts",
    "ios",
    "android",
    "tsconfig.json"
  ],
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    }
  },
  "peerDependencies": {
    "@expo/config-plugins": ">=9 <12",
    "react-native-app-auth": "^8.0.3"
  },
  "devDependencies": {
    "@expo/config-plugins": "^10.0.0",
    "typescript": "~5.8.3"
  },
  "engines": {
    "node": ">=18"
  }
}
